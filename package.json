{
  "name": "stylelint-warning-to-vscode-diagnostic",
  "version": "1.0.0",
  "description": "Convert a stylelint warning into a Visual Studio Code diagnostic",
  "repository": "shinnn/stylelint-warning-to-vscode-diagnostic",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint --fix --format=codeframe index.js test.js",
    "test": "nyc --reporter=html --reporter=text node --harmony_async_await test.js"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "stylelint",
    "vscode",
    "warning",
    "error",
    "diagnostic",
    "convert",
    "compatibility",
    "interface",
    "object",
    "severity"
  ],
  "devDependencies": {
    "@shinnn/eslint-config-node": "^3.0.0",
    "eslint": "^3.15.0",
    "nyc": "^10.1.2",
    "stylelint": "^7.8.0",
    "tape": "^4.6.3"
  },
  "eslintConfig": {
    "extends": "@shinnn/node"
  }
}
